<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kyu.tourrex">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Podroze"
            android:label="Podroze" />

        <activity android:name=".Natura"
            android:label="Natura" >
        </activity>

        <activity android:name=".Jedzenie"
            android:label="Jedzenie" />
        <activity android:name=".BasicActivity"
            android:label="Plan your trip!" />
    </application>

</manifest>